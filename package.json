  "readmeFilename": "README.md",
  "dependencies": {
    "hubot": ">=2.6.0",
    "request": "^2.88.0",
    "underscore": "~1.6.0",
    "ws": "^6.1.4"
  },
  "devDependencies": {
    "chai": "*",
    "coffee-coverage": "^3.0.1",
    "coffeescript": "~1.6.3",
    "coveralls": "^3.0.3",
